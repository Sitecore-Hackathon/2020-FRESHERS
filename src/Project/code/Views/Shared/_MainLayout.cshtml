@inherits WebViewPage
<!DOCTYPE html>
<!--[if IE 9]><html lang="en" class="ie9 no-js"><![endif]-->
<!--[if !IE]><!-->
<!--Started From Here-->

<html lang="@Sitecore.Context.Language.CultureInfo.TwoLetterISOLanguageName">

<head>

    <!-- metas -->
    <meta charset="utf-8">
    <meta name="author" content="hackathon" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <meta name="keywords" content="Sitecore Hackathon" />
    <meta name="description" content="Sitecore Hackathon - Competitive Agency With Creeative Ideas" />
    <!-- title  -->
    <title>@Html.Sitecore().Field("Title")</title>
    <!-- favicon -->
    <link rel="shortcut icon" href="~/assests/img/logos/favicon.png" />
    <link rel="apple-touch-icon" href="~/assests/img/logos/apple-touch-icon-57x57.png" />
    <link rel="apple-touch-icon" sizes="72x72" href="~/assests/img/logos/apple-touch-icon-72x72.png" />
    <link rel="apple-touch-icon" sizes="114x114" href="~/assests/img/logos/apple-touch-icon-114x114.png" />

    <!-- plugins -->

    <link href="~/assests/css/plugins.css" rel="stylesheet" />
    <!-- search css -->
    <link rel="stylesheet" href="~/assests/search/search.css" />

    <!-- core style css -->
    <link href="~/assests/css/styles.css" rel="stylesheet" id="colors" />

</head>

<body>

    <!-- start page loading -->
    <div id="preloader">
        <div class="row loader">
            <div class="loader-icon"></div>
        </div>
    </div>
    <!-- end page loading -->
    <!-- start main-wrapper section -->
    <div class="main-wrapper">

        <!-- Search header section -->
        <header class="position-absolute width-100 sm-position-relative">
            <div class="navbar-default">
                <!-- start top search -->
                <div class="top-search bg-theme">
                    <div class="container">
                        <form class="search-form" action="search.html" method="GET" accept-charset="utf-8">
                            <div class="input-group">
                                <span class="input-group-addon cursor-pointer">
                                    <button class="search-form_submit fas fa-search font-size18 text-white" type="submit"></button>
                                </span>
                                <input type="text" class="search-form_input form-control margin-10px-lr" name="s" autocomplete="off" placeholder="Type & hit enter...">
                                <span class="input-group-addon close-search"><i class="fas fa-times font-size18 margin-5px-top"></i></span>
                            </div>
                        </form>
                    </div>
                </div>
                <!-- end top search -->

                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-12 col-lg-12">
                            <div class="menu_area alt-font">
                                <nav class="navbar navbar-expand-lg navbar-light no-padding">

                                    <div class="navbar-header navbar-header-custom">
                                        <!-- start logo -->
                                        <a href="javascript:void(0)" class="navbar-brand">
                                            @Html.Sitecore().Field("Logo")
                                            @*<img id="logo" src="~/assests/img/logos/logo.png" alt="logo">*@
                                        </a>
                                        <!-- end logo -->
                                    </div>

                                    <div class="navbar-toggler"></div>

                                    <!-- start menu Area -->
                                    <ul class="navbar-nav ml-auto" id="nav">
                                        <li>
                                            <a href="javascript:void(0)">Home</a>
                                        </li>
                                        <li><a href="about.html">About Us</a></li>
                                        <li>
                                            <a href="javascript:void(0)">Pages</a>
                                            <ul>
                                                <li><a href="our-team.html">Our Team</a></li>
                                                <li><a href="faq.html">FAQ</a></li>
                                                <li><a href="projects.html">Projects</a></li>
                                                <li><a href="projects-style2.html">Projects Style2</a></li>
                                                <li><a href="single-project.html">Single Project</a></li>
                                                <li>
                                                    <a href="javascript:void(0)">Others</a>
                                                    <ul>
                                                        <li><a href="pricing.html">Pricing</a></li>
                                                        <li><a href="tables.html">Tables</a></li>
                                                        <li><a href="tabs-and-accordions.html">Tabs and Accordions</a></li>
                                                        <li><a href="404-page.html">Error 404</a></li>
                                                        <li><a href="coming-soon.html">Comingsoon</a></li>
                                                    </ul>
                                                </li>
                                            </ul>
                                        </li>

                                        <li><a href="services.html">Services</a></li>
                                        <li>
                                            <a href="javascript:void(0)">Blog</a>
                                            <ul>
                                                <li><a href="blog-grid.html">Blog Grid</a></li>
                                                <li><a href="blog-default.html">Blog Default</a></li>
                                                <li><a href="blog-post.html">Blog Post</a></li>
                                            </ul>
                                        </li>
                                        <li><a href="contact.html">Contact Us</a></li>
                                    </ul>
                                    <!-- end menu Area -->
                                    <!-- start attribute navigation -->
                                    <div class="attr-nav sm-no-margin sm-margin-65px-right">
                                        <ul>
                                            <li class="search"><a href="javascript:void(0)"><i class="fas fa-search"></i></a></li>
                                        </ul>
                                    </div>
                                    <!-- end attribute navigation -->

                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <!-- end header section -->
        <!-- start main banner area -->
        @Html.Sitecore().Placeholder("banner")
        <!-- end main banner area -->
        <!-- start service section -->
        <section class="service-section">
            <div class="container">
                <div class="text-center section-heading">
                    <div class="alt-font text-theme-color margin-5px-bottom letter-spacing-1">
                        @Html.Sitecore().Field("JourneyTitle")
                    </div>
                    <h2>@Html.Sitecore().Field("JourneyDescription")</h2>
                </div>
                @Html.Sitecore().Placeholder("journey-content")
            </div>
        </section>
        <!-- end service section -->
        <!-- start business section -->
        @Html.Sitecore().Placeholder("content")

        <section>
            <div class="container">
                <div class="section-heading">
                    <h3>Mailing List</h3>
                    <p class="width-55 sm-width-75 xs-width-95">
                        Use the form below to signup to our mailing list. The Hackathon updates will be sent out on a weekly basis, no more than 1 email per week. The email addresses will not be sold or used for any other purpose other than the Sitecore Hackathon related activities.
                    </p>
                </div>
                <div class="newsletter width-50 width-50 sm-width-75 xs-width-90 center-col">
                    <div class="input">
                        <input type="email" placeholder="Enter your email">
                        <a href="javascript:void(0)" class="butn"><span>Subscribe</span></a>
                    </div>
                </div>

            </div>
        </section>
        <!-- end newsletter section -->
        <!-- start footer section -->
        <!--Sitecore Footer Placeholder-->
        @Html.Sitecore().Placeholder("footer")

        <footer class="bg-theme-light">
            <div class="container">
                <div class="row">

                    <div class="col-lg-4 col-md-6 sm-margin-30px-bottom">
                        @Html.Sitecore().Field("Logo")
                        <p class="margin-20px-top text-extra-dark-gray">
                            Sitecore Hackathon returns for its 7th year in a row on Friday 8PM (EST), February 28th, 2020, to Saturday 8PM (EST), February 29th, 2020.
                        </p>
                        <div class="margin-25px-top xs-margin-20px-top footer-social-icons">
                            <ul>
                                <li><a href="@Html.Sitecore().Field("FacebookIcon")"><i class="fab fa-facebook-f"></i></a></li>
                                <li><a href="@Html.Sitecore().Field("TwitterIcon")"><i class="fab fa-twitter"></i></a></li>
                                <li><a href="@Html.Sitecore().Field("GooglePlusIcon")"><i class="fab fa-google-plus-g"></i></a></li>
                                <li><a href="@Html.Sitecore().Field("LinkinIdIcon")"><i class="fab fa-youtube"></i></a></li>
                                <li><a href="@Html.Sitecore().Field("LinkinIdIcon")"><i class="fab fa-linkedin-in"></i></a></li>
                            </ul>
                        </div>

                    </div>

                    <div class="col-lg-4 col-md-6 sm-margin-30px-bottom">
                        <h3 class="footer-title-style2 text-theme-color">Quick Links</h3>
                        <div class="row">
                            <div class="col-md-6 no-padding-right xs-padding-15px-right">
                                <ul class="footer-list xs-margin-5px-bottom">

                                    <li><a href="about.html">About us</a></li>
                                    <li><a href="services.html">Services</a></li>
                                    <li><a href="our-team.html">Our team</a></li>
                                    <li><a href="contact.html">Contact us</a></li>
                                    <li><a href="blog-default.html">Blog</a></li>
                                </ul>
                            </div>
                            <div class="col-md-6 no-padding-right xs-padding-15px-right">
                                <ul class="footer-list">

                                    <li><a href="projects.html">Projects</a></li>
                                    <li><a href="pricing.html">Pricing</a></li>
                                    <li><a href="faq.html">FAQ</a></li>
                                    <li>
                                        <a href="404-page.html"></a>Error 404
                                    </li>
                                    <li><a href="blog-post.html">Blog post</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6 offset-lg-1">
                        <h3 class="footer-title-style2 text-theme-color">Get in Touch</h3>
                        <ul class="footer-list">
                            <li>
                                <span class="d-inline-block vertical-align-top font-size18"><i class="fas fa-map-marker-alt text-theme-color"></i></span>
                                <span class="d-inline-block width-85 vertical-align-top padding-10px-left">@Html.Sitecore().Field("OurAddress")</span>
                            </li>
                            <li>
                                <span class="d-inline-block vertical-align-top font-size18"><i class="fas fa-mobile-alt text-theme-color"></i></span>
                                <span class="d-inline-block width-85 vertical-align-top padding-10px-left">@Html.Sitecore().Field("ContactNo")</span>
                            </li>
                            <li>
                                <span class="d-inline-block vertical-align-top font-size18"><i class="far fa-envelope text-theme-color"></i></span>
                                <span class="d-inline-block width-85 vertical-align-top padding-10px-left">@Html.Sitecore().Field("Email")</span>
                            </li>
                            <li>
                                <span class="d-inline-block vertical-align-top font-size18"><i class="fas fa-globe text-theme-color"></i></span>
                                <span class="d-inline-block width-85 vertical-align-top padding-10px-left">@Html.Sitecore().Field("WebsiteUrl")</span>
                            </li>
                        </ul>
                    </div>

                </div>

            </div>
            <div class="footer-bar xs-font-size13">
                <div class="container">
                    <div class="row">
                        <div class="col-md-6 text-left xs-text-center xs-margin-5px-bottom">
                            <p>&copy;@Html.Sitecore().Field("CopyrightText")</p>
                        </div>
                        <div class="col-md-6 text-right xs-text-center">
                            @Html.Sitecore().Field("AgencyText")
                        </div>
                    </div>
                </div>
            </div>
        </footer>
        <!-- end footer section -->

    </div>
    <!-- end main-wrapper section -->
    <!-- start scroll to top -->
    <a href="javascript:void(0)" class="scroll-to-top">
        <i class="fas fa-angle-up" aria-hidden="true"></i>
    </a>
    <!-- end scroll to top -->
    <!-- all js include start -->
    <!-- jQuery -->

    <script src="~/assests/js/jquery.min.js"></script>

    <!-- modernizr js -->
    <script src="~/assests/js/modernizr.js"></script>

    <!-- bootstrap -->
    <script src="~/assests/js/bootstrap.min.js"></script>

    <!-- scrollIt -->
    <script src="~/assests/js/scrollIt.min.js"></script>

    <!-- Serch -->
    <script src="~/assests/search/search.js"></script>

    <!-- navigation -->
    <script src="~/assests/js/nav-menu.js"></script>

    <!-- tab -->
    <script src="~/assests/js/easy.responsive.tabs.js"></script>

    <!-- owl carousel -->
    <script src="~/assests/js/owl.carousel.js"></script>

    <!-- jquery.counterup.min -->
    <script src="~/assests/js/jquery.counterup.min.js"></script>

    <!-- stellar js -->
    <script src="~/assests/js/jquery.stellar.min.js"></script>

    <!-- waypoints js -->
    <script src="~/assests/js/waypoints.min.js"></script>

    <!-- tab js -->
    <script src="~/assests/js/tabs.min.js"></script>

    <!-- countdown js -->
    <script src="~/assests/js/countdown.js"></script>

    <!-- jquery.magnific-popup js -->
    <script src="~/assests/js/jquery.magnific-popup.min.js"></script>

    <!-- isotope.pkgd.min js -->
    <script src="~/assests/js/isotope.pkgd.min.js"></script>

    <!-- wow js -->
    <script src="~/assests/js/wow.js"></script>

    <!-- map js -->
    <script src="~/assests/js/map.js"></script>

    <!-- custom scripts -->
    <script src="~/assests/js/main.js"></script>

    <!-- contact form scripts -->
    <script src="~/assests/js/mailform/jquery.form.min.js"></script>
    <script src="~/assests/js/mailform/jquery.rd-mailform.min.c.js"></script>

    <!-- all js include end -->

</body>

</html>